User
    ident Text
    UniqueUser ident
    admin Bool
    displayName Text Maybe
    deriving Typeable
    deriving Show
Cube
    creator UserId
    name Text
    UniqueCubeName name
    deriving Show
CubeEntry
    cube CubeId
    card CardId
    Primary cube card
    deriving Show
Card
    Primary card
    card Text
    colors [Text]
    -- ie instant, land
    -- not super types like legendary or subtypes like human
    types [Text]
    UniqueCardName card
    deriving Show
Draft
    creator UserId
    cube CubeId
    participants [UserId]
    rounds Word default=45
    created UTCTime
    deriving Show
Pick
    draft DraftId
    number Int -- starts at 0, ascends for each pick
    Primary draft number
    cube CubeId
    card CardId
    Foreign CubeEntry asdf cube card
    drafter UserId
    UniquePick draft number
    created UTCTime
    deriving Show

 -- By default this file is used in Model.hs (which is imported by Foundation.hs)
